{
  "private": true,
  "name": "uiengine-sample-project",
  "description": "UIengine Sample Project",
  "license": "MIT",
  "version": "0.14.0",
  "author": "Dennis Reimann <mail@dennisreimann.de> (https://dennisreimann.de)",
  "homepage": "https://github.com/dennisreimann/uiengine",
  "bugs": "https://github.com/dennisreimann/uiengine",
  "repository": "dennisreimann/uiengine",
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "clean": "rimraf ../tmp",
    "setup": "yarn install && yarn link-packages",
    "start": "yarn run clean && gulp develop",
    "build": "yarn run clean && gulp build",
    "lint": "eslint --ext .js,.vue src test",
    "link-packages": "for pkg in $(ls -1FA ../../packages | grep / | tr -d /); do yarn link $pkg; done",
    "standalone": "yarn run clean && uiengine build -s -w -d"
  },
  "dependencies": {
    "ejs": "^2.5.8",
    "handlebars": "^4.0.11",
    "marko": "^4.9.3",
    "pug": "^2.0.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "vue": "^2.5.16",
    "vue-server-renderer": "^2.5.16"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-eslint": "^8.2.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-vue": "^2.0.2",
    "babel-register": "^6.26.0",
    "browser-sync": "^2.23.6",
    "connect-history-api-fallback": "^1.5.0",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "eslint": "4.19.1",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-promise": "3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "gulp": "^3.9.1",
    "html-sketchapp-cli": "0.3.0",
    "theo": "^6.0.0-beta.9",
    "uiengine": "file:../../packages/uiengine",
    "uiengine-adapter-ejs": "file:../../packages/uiengine-adapter-ejs",
    "uiengine-adapter-handlebars": "file:../../packages/uiengine-adapter-handlebars",
    "uiengine-adapter-html": "file:../../packages/uiengine-adapter-html",
    "uiengine-adapter-marko": "file:../../packages/uiengine-adapter-marko",
    "uiengine-adapter-pug": "file:../../packages/uiengine-adapter-pug",
    "uiengine-adapter-react": "file:../../packages/uiengine-adapter-react",
    "uiengine-adapter-vue": "file:../../packages/uiengine-adapter-vue",
    "uiengine-theme-default": "file:../../packages/uiengine-theme-default",
    "vue-loader": "^14.2.2",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.3.0",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.6.0",
    "webpack-stream": "^4.0.3"
  }
}
