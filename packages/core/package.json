{
  "name": "uiengine",
  "description": "Create, document and evolve your design system collaboratively.",
  "license": "MIT",
  "version": "0.10.0",
  "author": "Dennis Reimann <mail@dennisreimann.de>",
  "homepage": "https://github.com/dennisreimann/uiengine",
  "bugs": "https://github.com/dennisreimann/uiengine",
  "repository": "dennisreimann/uiengine",
  "engines": {
    "node": ">=6.11.0"
  },
  "main": "./lib/uiengine.js",
  "bin": {
    "uiengine": "./bin/uiengine.js"
  },
  "files": [
    "bin",
    "lib"
  ],
  "keywords": [
    "uiengine",
    "uiengineering",
    "pattern-library",
    "style-guide",
    "static-site"
  ],
  "scripts": {
    "clean": "rimraf lib test/tmp",
    "start": "yarn run clean && yarn run build -- -w & yarn run test -- -w",
    "build": "yarn run clean && babel src --out-dir lib",
    "test": "mocha --compilers js:babel-register -R dot",
    "cover": "nyc yarn run test"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "deep-extend": "^0.5.0",
    "fs-extra": "^3.0.1",
    "globby": "^6.1.0",
    "js-yaml": "^3.7.0",
    "markdown-it": "^8.2.2",
    "markdown-it-anchor": "^4.0.0",
    "ramda": "^0.24.1",
    "uiengine-theme-default": "^0.10.0",
    "yargs": "^8.0.2"
  },
  "optionalDependencies": {
    "browser-sync": "^2.18.8",
    "chokidar": "^1.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-register": "^6.23.0",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "mocha-sinon": "^2.0.0",
    "nyc": "^11.0.3",
    "sinon": "^2.3.5"
  }
}
