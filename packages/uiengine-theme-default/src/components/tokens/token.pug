mixin token(token)
  - const { name, description, value, variable, reference } = token
  tr.defaultToken&attributes(attributes)
    td.defaultToken__value= name
    td.defaultToken__value.defaultToken__value--value= value
    td.defaultToken__value.defaultToken__value--variable= variable
    td.defaultToken__value= description
    td.defaultToken__value= reference
