.uie
  $fontsizes = json(pathjoin('tokens', 'fontsizes.json'), { hash: true })
  for $name, $value in $fontsizes
    .fs-{$name}
      font-size $value

  $spaces = json(pathjoin('tokens', 'spaces.json'), { hash: true })
  for $name, $value in $spaces
    .sot-{$name}
      margin-top $value
    .sob-{$name}
      margin-bottom $value
